{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "pip install -r requirements.txt"
  },
  "deploy": {
    "startCommand": "gunicorn honeypot_site.wsgi:application --workers 1 --timeout 120 --bind 0.0.0.0:$PORT --log-file -",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100
  }
}